# ALIAS
alias love="/Applications/love.app/Contents/MacOS/love"

# PATHS
export PATH="$PATH:/usr/local/mysql/bin"
export PATH="$HOME/.rbenv/bin:$PATH"

# RBENV
export GEM_HOME=~/.gems

# Git aliases
balias gst 'git status'
balias gl 'git log'
balias ga 'git add'
balias gcm 'git commit -m'
